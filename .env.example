# AEGIS Protocol Environment Configuration
# Copy this file to .env and fill in the values

# Deployment mode for dfx-replica service
# 'auto' = automatically start replica and deploy canisters (recommended for development)
# 'manual' = require manual deployment via docker exec (for production or debugging)
DEPLOY_MODE=auto

# ID Canister dari EventFactory di Internet Computer.
# This will be automatically filled after deployment
EVENT_FACTORY_CANISTER_ID=
